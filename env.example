# AgentOS API Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/database_name
POSTGRES_DB=ai_cost_tracker
POSTGRES_USER=your_db_user
POSTGRES_PASSWORD=your_secure_db_password

# Redis Configuration  
REDIS_URL=redis://localhost:6379

# API Security
JWT_SECRET=your-super-secure-jwt-secret-key-min-32-characters
API_KEY_PREFIX=ak_

# OpenAI API (for testing)
OPENAI_API_KEY=sk-proj-your-openai-key-here

# Anthropic API (for testing)
ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here

# AgentOS API Authentication
API_KEY=your-api-key-here
AUTH_TOKEN=your-api-key-here
AGENTOS_KEY=your-api-key-here

# Development Settings
NODE_ENV=development
PORT=3000
API_BASE_URL=http://localhost:3000

# Frontend (Dashboard)
VITE_API_URL=http://localhost:3000

# Security Settings (Production)
CORS_ORIGIN=https://your-domain.com
RATE_LIMIT_WINDOW=15
RATE_LIMIT_MAX=100
